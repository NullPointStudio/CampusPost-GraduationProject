M,GAAG,IAAIb,SAAJ,CAAc;AAC3BO,EAAAA,MAAM,EAANA;AAD2B,CAAd,CAAf,C,CAIA;;AACAM,MAAM,CAACC,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC;AACA;AACA;AACA;AACA,MAAIF,EAAE,CAACP,IAAH,KAAY,QAAhB,EAA0B,OAAOS,IAAI,EAAX,CALU,CAMpC;;AACA,MAAMC,QAAQ,GAAGC,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,OAA9B,CAAjB;;AACA,MAAI,CAACH,QAAL,EAAe;AACb,IAAA,KAAI,CAACI,QAAL,CAAcC,KAAd,CAAoB,MAApB;;AACA,WAAON,IAAI,CAAC,QAAD,CAAX;AACD;;AACDA,EAAAA,IAAI;AACL,CAbD;AAeA,eAAeJ,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport Login from '@/components/Login'\nimport Manage from '@/components/Manage'\nimport ManageMenu from '@/components/ManageMenu'\nimport StudentManager from '@/components/manage/StudentManager'\nimport TeacherManager from '@/components/manage/TeacherManager'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    redirect: '/login'\n  },\n  {\n    path: '/login',\n    name: 'Home',\n    component: Login\n  },\n  {\n    path: '/manage',\n    component: Manage,\n    children: [\n      {\n        path: '/manageMenu',\n        component: ManageMenu,\n        children: [{\n          path: '/teacherManager',\n          component: TeacherManager\n        }, {\n          path: '/studentManager',\n          component: StudentManager\n        }\n        ]\n      }\n    ]\n  }\n]\n\nconst router = new VueRouter({\n  routes\n})\n\n// 挂在路由导航守卫\nrouter.beforeEach((to, from, next) => {\n  // to 将要访问的路径\n  // from 代表从哪个路径跳转而来\n  // next 是一个函数，表示放行\n  // next('/login) 表示强制跳转到login页面\n  if (to.path === '/login') return next()\n  // 获取token\n  const tokenStr = window.sessionStorage.getItem('token')\n  if (!tokenStr) {\n    this.$message.error('请先登录')\n    return next('/login')\n  }\n  next()\n})\n\nexport default router\n"]}]}