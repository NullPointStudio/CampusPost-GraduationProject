,SAAS,EAAEd,MAFb;AAGEe,EAAAA,QAAQ,EAAE,CACR;AACEJ,IAAAA,IAAI,EAAE,aADR;AAEEG,IAAAA,SAAS,EAAEb,UAFb;AAGEc,IAAAA,QAAQ,EAAE,CAAC;AACTJ,MAAAA,IAAI,EAAE,iBADG;AAETG,MAAAA,SAAS,EAAEX;AAFF,KAAD,EAGP;AACDQ,MAAAA,IAAI,EAAE,iBADL;AAEDG,MAAAA,SAAS,EAAEZ;AAFV,KAHO,EAMP;AACDS,MAAAA,IAAI,EAAE,eADL;AAEDG,MAAAA,SAAS,EAAET;AAFV,KANO,EASP;AACDM,MAAAA,IAAI,EAAE,mBADL;AAEDG,MAAAA,SAAS,EAAER;AAFV,KATO,EAYP;AACDK,MAAAA,IAAI,EAAE,wBADL;AAEDG,MAAAA,SAAS,EAAEP;AAFV,KAZO,EAeP;AACDI,MAAAA,IAAI,EAAE,oBADL;AAEDG,MAAAA,SAAS,EAAEN;AAFV,KAfO,EAkBP;AACDG,MAAAA,IAAI,EAAE,WADL;AAEDG,MAAAA,SAAS,EAAEV;AAFV,KAlBO;AAHZ,GADQ;AAHZ,CAVa,CAAf;AA6CA,IAAMY,MAAM,GAAG,IAAIlB,SAAJ,CAAc;AAC3BY,EAAAA,MAAM,EAANA;AAD2B,CAAd,CAAf;AAGA,IAAMO,aAAa,GAAGnB,SAAS,CAACoB,SAAV,CAAoBC,IAA1C;;AACArB,SAAS,CAACoB,SAAV,CAAoBC,IAApB,GAA2B,SAASA,IAAT,CAAeC,EAAf,EAAmB;AAC5C,SAAOH,aAAa,CAACI,IAAd,CAAmB,IAAnB,EAAyBD,EAAzB,EAA6BE,KAA7B,CAAmC,UAAAC,GAAG;AAAA,WAAIA,GAAJ;AAAA,GAAtC,CAAP;AACD,CAFD,C,CAIA;;;AACAP,MAAM,CAACQ,UAAP,CAAkB,UAACJ,EAAD,EAAKK,IAAL,EAAWC,IAAX,EAAoB;AACpC;AACA;AACA;AACA;AACA,MAAIN,EAAE,CAACT,IAAH,KAAY,QAAhB,EAA0B,OAAOe,IAAI,EAAX,CALU,CAMpC;;AACA,MAAMC,QAAQ,GAAGC,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,OAA9B,CAAjB;;AACA,MAAI,CAACH,QAAL,EAAe;AACb,IAAA,KAAI,CAACI,QAAL,CAAcC,KAAd,CAAoB,MAApB;;AACA,WAAON,IAAI,CAAC,QAAD,CAAX;AACD;;AACDA,EAAAA,IAAI;AACL,CAbD;AAeA,eAAeV,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport Login from '@/components/Login'\nimport Manage from '@/components/Manage'\nimport ManageMenu from '@/components/ManageMenu'\nimport StudentManager from '@/components/manage/StudentManager'\nimport TeacherManager from '@/components/manage/TeacherManager'\nimport ViewsNum from '@/components/manage/ViewsNum'\nimport ClassManager from '@/components/manage/ClassManager'\nimport DormitoryManager from '@/components/manage/DormitoryManager'\nimport DormitoryDistribution from '@/components/manage/DormitoryDistribution'\nimport ClassDistribution from '@/components/manage/ClassDistribution'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    redirect: '/login'\n  },\n  {\n    path: '/login',\n    name: 'Home',\n    component: Login\n  },\n  {\n    path: '/manage',\n    component: Manage,\n    children: [\n      {\n        path: '/manageMenu',\n        component: ManageMenu,\n        children: [{\n          path: '/teacherManager',\n          component: TeacherManager\n        }, {\n          path: '/studentManager',\n          component: StudentManager\n        }, {\n          path: '/classManager',\n          component: ClassManager\n        }, {\n          path: '/dormitoryManager',\n          component: DormitoryManager\n        }, {\n          path: '/dormitoryDistribution',\n          component: DormitoryDistribution\n        }, {\n          path: '/classDistribution',\n          component: ClassDistribution\n        }, {\n          path: '/viewsNum',\n          component: ViewsNum\n        }\n        ]\n      }\n    ]\n  }\n]\n\nconst router = new VueRouter({\n  routes\n})\nconst VueRouterPush = VueRouter.prototype.push\nVueRouter.prototype.push = function push (to) {\n  return VueRouterPush.call(this, to).catch(err => err)\n}\n\n// 挂在路由导航守卫\nrouter.beforeEach((to, from, next) => {\n  // to 将要访问的路径\n  // from 代表从哪个路径跳转而来\n  // next 是一个函数，表示放行\n  // next('/login) 表示强制跳转到login页面\n  if (to.path === '/login') return next()\n  // 获取token\n  const tokenStr = window.sessionStorage.getItem('token')\n  if (!tokenStr) {\n    this.$message.error('请先登录')\n    return next('/login')\n  }\n  next()\n})\n\nexport default router\n"]}]}