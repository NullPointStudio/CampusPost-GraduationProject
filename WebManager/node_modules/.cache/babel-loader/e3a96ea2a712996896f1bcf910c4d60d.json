;AAAA;;AAAA;;AAAA;AAAA;AAAA,yBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,4BAAA,EAAA,MAAA,CAAA,eAAA,CAFA;;AAAA;AAAA;AAEA,kBAAA,GAFA,yBAEA,IAFA;;AAAA,wBAGA,GAAA,CAAA,IAAA,KAAA,GAHA;AAAA;AAAA;AAAA;;AAIA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,kBAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;AALA;AAAA,yBAMA,MAAA,CAAA,cAAA,EANA;;AAAA;AAAA;AAAA;;AAAA;AAQA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,GAAA,CAAA,GAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAWA,KA/BA;AAgCA;AACA,IAAA,gBAjCA,4BAiCA,OAjCA,EAiCA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,OAAA;AACA,WAAA,cAAA;AACA,KApCA;AAqCA;AACA,IAAA,mBAtCA,+BAsCA,OAtCA,EAsCA;AACA,WAAA,SAAA,CAAA,OAAA,GAAA,OAAA;AACA,WAAA,cAAA;AACA,KAzCA;AA0CA;AACA,IAAA,UA3CA,wBA2CA;AACA,WAAA,cAAA;AACA,KA7CA;AA8CA,IAAA,YA9CA,wBA8CA,GA9CA,EA8CA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,eAAA,CAAA,EAAA,GAAA,GAAA,CAAA,UAAA,GAAA,EAAA;AACA,WAAA,eAAA,CAAA,IAAA,GAAA,GAAA,CAAA,QAAA;AACA,WAAA,eAAA,CAAA,GAAA,GAAA,GAAA,CAAA,GAAA;AACA,WAAA,eAAA,CAAA,KAAA,GAAA,GAAA,CAAA,KAAA;AACA,WAAA,eAAA,CAAA,UAAA,GAAA,GAAA,CAAA,UAAA;AACA,KArDA;AAsDA,IAAA,eAtDA,6BAsDA;AACA,WAAA,KAAA,CAAA,kBAAA,CAAA,WAAA;AACA,KAxDA;AAyDA,IAAA,WAzDA,yBAyDA;AAAA;;AACA,WAAA,KAAA,CAAA,kBAAA,CAAA,QAAA;AAAA,4EAAA,kBAAA,KAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KADA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAAA;AAAA,yBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,6BAAA,EAAA,MAAA,CAAA,eAAA,CAFA;;AAAA;AAAA;AAEA,kBAAA,GAFA,yBAEA,IAFA;;AAAA,wBAGA,GAAA,CAAA,IAAA,KAAA,GAHA;AAAA;AAAA;AAAA;;AAIA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,kBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AALA;AAAA,yBAMA,MAAA,CAAA,cAAA,EANA;;AAAA;AAAA;AAAA;;AAAA;AAQA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,GAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAYA,KAtEA;AAuEA,IAAA,aAvEA,yBAuEA,EAvEA,EAuEA;AAAA;;AACA,WAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,uEAIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,wBAAA,EAAA;AAAA,kBAAA,EAAA,EAAA,EAAA,GAAA;AAAA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAFA,sBAGA,GAAA,CAAA,IAAA,KAAA,GAHA;AAAA;AAAA;AAAA;;AAAA,kDAIA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAJA;;AAAA;AAMA,gBAAA,MAAA,CAAA,QAAA,CAAA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AANA;AAAA,uBAUA,MAAA,CAAA,cAAA,EAVA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAJA,IAgBA,KAhBA,CAgBA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OArBA;AAsBA;AA9FA;AAjGA,CAAA","sourcesContent":["<template>\r\n  <!-- 面包屑导航区域 -->\r\n  <div>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item>首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>学生管理</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- card -->\r\n    <el-card class=\"box-card\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"7\">\r\n          <!-- input搜做框 -->\r\n          <el-input placeholder=\"请输入学生姓名查询\" clearable @clear=\"selectList\" v-model=\"queryInfo.query\"\r\n                    class=\"input-with-select\">\r\n            <el-button slot=\"append\" type=\"primary\" @click=\"selectList\" icon=\"el-icon-search\"></el-button>\r\n          </el-input>\r\n        </el-col>\r\n        <el-col :span=\"4\">\r\n          <el-button type=\"primary\" @click=\"addDialogVisible = true\">添加学生</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <el-table\r\n        :data=\"teacherList\"\r\n        border\r\n        style=\"width: 1122px\">\r\n        <!-- 索引列 -->\r\n        <el-table-column type=\"index\" label=\"#\" width=\"80\"/>\r\n        <el-table-column prop=\"account_id\" label=\"账号Id\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"turename\" label=\"姓名\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"sex\" label=\"性别\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"college_id\" label=\"所属二级学院名称\" width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <span>\r\n<!--                {{ scope.row.college_id | formduty }}-->\r\n                {{ options[scope.row.college_id - 1].label }}\r\n            </span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"phone\" label=\"手机号\" width=\"150\"></el-table-column>\r\n        <el-table-column prop=\"dormitory_name\" label=\"宿舍\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"class_name\" label=\"班级\" width=\"100\"></el-table-column>\r\n        <el-table-column\r\n          label=\"操作\"\r\n          width=\"190\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button @click=\"editTableBtn(scope.row)\" type=\"warning\" size=\"mini\" icon=\"el-icon-edit\">编辑</el-button>\r\n            <el-button @click=\"deleteTeacher(scope.row.account_id)\" type=\"danger\" size=\"mini\" icon=\"el-icon-delete\">删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pageNum\"\r\n        :page-sizes=\"[2, 8, 32, 64]\"\r\n        :page-size=\"queryInfo.pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n    <!--  Dialog添加对话框  -->\r\n    <el-dialog\r\n      title=\"添加学生\"\r\n      :visible.sync=\"addDialogVisible\"\r\n      width=\"30%\"\r\n      @close=\"addDialogClose\">\r\n      <!-- 内容主题区域  -->\r\n      <el-form :model=\"teacherInfoForm\" :rules=\"teacherInfoFormRules\" ref=\"teacherInfoFormRef\" label-width=\"70px\">\r\n        <el-form-item label=\"姓名\" prop=\"name\">\r\n          <el-input placeholder=\"请输入姓名\" v-model=\"teacherInfoForm.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-radio v-model=\"teacherInfoForm.sex\" label=\"男\">男</el-radio>\r\n          <el-radio v-model=\"teacherInfoForm.sex\" label=\"女\">女</el-radio>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机号\" prop=\"phone\">\r\n          <el-input placeholder=\"手机号\" v-model=\"teacherInfoForm.phone\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"二级学院\">\r\n          <el-select v-model=\"teacherInfoForm.select_college\" placeholder=\"请选择二级学院名称\">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <!-- 底部区域 -->\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addTeacher\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog\r\n      title=\"修改学生信息\"\r\n      :visible.sync=\"editDialogVisible\"\r\n      width=\"30%\"\r\n      @close=\"editDialogClose\">\r\n      <!-- 内容主题区域  -->\r\n      <el-form :model=\"teacherEditForm\" :rules=\"teacherEditFormRules\" ref=\"teacherEditFormRef\" label-width=\"70px\">\r\n        <el-form-item label=\"账号ID\" prop=\"phone\">\r\n          <el-input placeholder=\"姓名\" disabled v-model=\"teacherEditForm.id\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"姓名\" prop=\"phone\">\r\n          <el-input placeholder=\"姓名\" v-model=\"teacherEditForm.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机号\" prop=\"phone\">\r\n          <el-input placeholder=\"手机号\" v-model=\"teacherEditForm.phone\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-radio v-model=\"teacherEditForm.sex\" label=\"男\">男</el-radio>\r\n          <el-radio v-model=\"teacherEditForm.sex\" label=\"女\">女</el-radio>\r\n        </el-form-item>\r\n        <el-form-item label=\"二级学院\">\r\n          <el-select v-model=\"teacherEditForm.select_college\" placeholder=\"请选择二级学院名称\">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <!-- 底部区域 -->\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"editTeacher\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'StudentManager',\r\n  created () {\r\n    this.getTeacherList()\r\n  },\r\n  data () {\r\n    return {\r\n      addDialogVisible: false,\r\n      editDialogVisible: false,\r\n      teacherInfoForm: {\r\n        name: '',\r\n        phone: '',\r\n        sex: '男',\r\n        select_college: 1\r\n      },\r\n      teacherEditForm: {\r\n        id: '',\r\n        name: '',\r\n        phone: '',\r\n        sex: '男',\r\n        select_college: 1\r\n      },\r\n      teacherEditFormRules: {\r\n        name: [\r\n          {\r\n            required: true,\r\n            message: '请输入姓名',\r\n            trigger: 'blur'\r\n          }\r\n        ],\r\n        phone: [\r\n          {\r\n            required: true,\r\n            message: '请输入手机号',\r\n            trigger: 'blur'\r\n          },\r\n          {\r\n            min: 11,\r\n            max: 11,\r\n            message: '请输入正确的手机号',\r\n            trigger: 'blur'\r\n          }\r\n        ]\r\n      },\r\n      teacherInfoFormRules: {\r\n        name: [\r\n          {\r\n            required: true,\r\n            message: '请输入姓名',\r\n            trigger: 'blur'\r\n          }\r\n        ],\r\n        phone: [\r\n          {\r\n            required: true,\r\n            message: '请输入手机号',\r\n            trigger: 'blur'\r\n          },\r\n          {\r\n            min: 11,\r\n            max: 11,\r\n            message: '请输入正确的手机号',\r\n            trigger: 'blur'\r\n          }\r\n        ]\r\n      },\r\n\r\n      options: [\r\n        {\r\n          value: 1,\r\n          label: '电子与信息学院'\r\n        }, {\r\n          value: 2,\r\n          label: '经济与贸易学院'\r\n        }, {\r\n          value: 3,\r\n          label: '世博艺术与传媒学院'\r\n        }, {\r\n          value: 4,\r\n          label: '建筑工程与管理学院'\r\n        }, {\r\n          value: 5,\r\n          label: '电商与物流学院'\r\n        }, {\r\n          value: 6,\r\n          label: '会计与金融学院'\r\n        }\r\n      ],\r\n      queryInfo: {\r\n        query: '',\r\n        pageNum: 1,\r\n        pageSize: 8\r\n      },\r\n      teacherList: [],\r\n      total: 0\r\n    }\r\n  },\r\n  methods: {\r\n    async getTeacherList () {\r\n      const { data: res } = await this.$http.get('/student/getStudentList', { params: this.queryInfo })\r\n      if (res.code === 200) {\r\n        this.teacherList = res.pageInfo.list\r\n        this.total = res.pageInfo.total\r\n        this.pageSize = res.pageInfo.pageSize\r\n        console.log(this.teacherList)\r\n      } else {\r\n        this.$message.error('获取学生列表失败')\r\n      }\r\n    },\r\n    // 当dialog关闭的时候出发的操作\r\n    addDialogClose () {\r\n      this.teacherInfoForm.select_college = 1\r\n      this.teacherInfoForm.sex = '男'\r\n      this.$refs.teacherInfoFormRef.resetFields()\r\n    },\r\n    // 添加教师\r\n    addTeacher () {\r\n      this.$refs.teacherInfoFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        const { data: res } = await this.$http.post('/student/addStudentByAdmin', this.teacherInfoForm)\r\n        if (res.code === 200) {\r\n          this.$message.success('添加成功!')\r\n          this.addDialogVisible = false\r\n          await this.getTeacherList()\r\n        } else {\r\n          this.$message.error('添加失败：' + res.msg)\r\n        }\r\n      })\r\n    },\r\n    // pageSize改变\r\n    handleSizeChange (newSize) {\r\n      this.queryInfo.pageSize = newSize\r\n      this.getTeacherList()\r\n    },\r\n    // 页码值改变\r\n    handleCurrentChange (newPage) {\r\n      this.queryInfo.pageNum = newPage\r\n      this.getTeacherList()\r\n    },\r\n    // 搜索教师名称\r\n    selectList () {\r\n      this.getTeacherList()\r\n    },\r\n    editTableBtn (row) {\r\n      this.editDialogVisible = true\r\n      this.teacherEditForm.id = row.account_id + ''\r\n      this.teacherEditForm.name = row.turename\r\n      this.teacherEditForm.sex = row.sex\r\n      this.teacherEditForm.phone = row.phone\r\n      this.teacherEditForm.college_id = row.college_id\r\n    },\r\n    editDialogClose () {\r\n      this.$refs.teacherEditFormRef.resetFields()\r\n    },\r\n    editTeacher () {\r\n      this.$refs.teacherEditFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        const { data: res } = await this.$http.post('/student/editStudentByAdmin', this.teacherEditForm)\r\n        if (res.code === 200) {\r\n          this.$message.success('修改成功!')\r\n          this.editDialogVisible = false\r\n          await this.getTeacherList()\r\n        } else {\r\n          console.log(res)\r\n          this.$message.error('修改失败：' + res.msg)\r\n        }\r\n      })\r\n    },\r\n    deleteTeacher (id) {\r\n      this.$confirm('此操作将永久删除该学生, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(async () => {\r\n        const { data: res } = await this.$http.post('/student/deleteStudent', { id: id + '' })\r\n        console.log(res)\r\n        if (res.code !== 200) {\r\n          return this.$message.error('删除失败')\r\n        } else {\r\n          this.$message({\r\n            type: 'success',\r\n            message: '删除成功!'\r\n          })\r\n          await this.getTeacherList()\r\n        }\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消'\r\n        })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-table {\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n  border: #e2e2e2 1px solid;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/manage"}]}